{
  "name": "fullstack-interview",
  "version": "1.0.0",
  "description": "Full-stack developer interview testing environment",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "node backend/server.js",
    "dev:backend": "nodemon backend/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "test": "echo \"Run 'npm run dev:all' to start both frontend and backend for testing\""
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}